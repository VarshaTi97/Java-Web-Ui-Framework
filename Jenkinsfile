pipeline {
  agent any {
    stages {
      stage("build") {
        steps {
          echo "This is build step"
        }
      }
    stage("test") {
        steps {
          echo "This is test step"
        }
      }
    stage("deploy") {
        steps {
          echo "This is deploy step"
        }
      }
    }
  
  }
}
